{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "targets": {
          "browsers": [
            "> 0.5%",
            "last 2 versions"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-proto-to-assign",
    ["@babel/plugin-transform-classes"],
    [
      "transform-es2015-classes",
      {
        "loose": true
      }
    ]
  ]
}